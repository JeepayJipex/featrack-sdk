{
  "name": "@jeepayjipex/featrack-sdk",
  "type": "module",
  "author": "JeepayJipex",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https//github.com/JeepayJipex/featrack-sdk.git"
    },
  "version": "0.0.2",
  "private": false,
  "files": [
    "dist",
    "package.json"
  ],
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "require": "./dist/index.cjs",
      "import": "./dist/index.js"
    },
    "./js": {
      "types": "./dist/js.d.ts",
      "require": "./dist/js.cjs",
      "import": "./dist/js.js"
    },
    "./*": "./*"
  },
  "scripts": {
    "start:dev": "vite",
    "dev": "tsup --tsconfig tsconfig.build.json --watch",
    "build": "tsup --tsconfig tsconfig.build.json",
    "preview": "vite preview"
  },
  "devDependencies": {
    "typescript": "~5.6.2",
    "vite": "^5.4.9",
    "tsup": "^8.3.5",
    "vite-plugin-dts": "^4.3.0"
  },
  "dependencies": {
    "axios": "^1.7.7"
  }
}